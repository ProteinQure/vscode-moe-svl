{
	"$schema": "https://raw.githubusercontent.com/martinring/tmlanguage/master/tmlanguage.json",
	"name": "Scientific Vector Language",
	"patterns": [
		{
			"include": "#keywords"
		},
		{
			"include": "#strings"
		}
	],
	"repository": {
		"keywords": {
			"patterns": [
				{
					"name": "keyword.control.svl",
					"match": "\\b(break|continue|else|elseif|for|goto|in|return|then|until|while)\\b"
				},
				{
					"name": "keyword.operator.svl",
					"match": "\\b(and|or)\\b"
				},
				{
					"name": "keyword.other.svl",
					"match": "\\b(app|apt|const|export|global|import|in|let|local|print|pr|static|var)\\b"
				}
			]
		},
		"strings": {
			"name": "string.quoted.double.svl",
			"begin": "\"",
			"end": "\"",
			"patterns": [
				{
					"name": "constant.character.escape.svl",
					"match": "\\\\."
				}
			]
		}
	},
	"scopeName": "source.svl"
}